#!/usr/bin/env ruby -> syntax higlight

# adding cache keyword before def tells spyon to cache the function result
# very much useful during recursion eg: fibonacci
cache def add_me(a,b) do
  # assume there is lot going here for 3 sec
	sleep(3000)
	return a+b
end


def main() do
	add_me(10,20):println()
	println(add_me(10,20))
end
